{
	"name": "mail-export",
	"description": "Parse .eml and .msg files or convert to pdf, html, jpeg or png format. Extract headers and attachments from .eml and msg files. Natively in typescript, support mjs & cjs!",
	"version": "1.0.0",
	"private": false,
	"type": "commonjs",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "tsup",
		"tsc": "tsc --noEmit --skipLibCheck"
	},
	"dependencies": {
		"tsup": "^8.2.4"
	},
	"devDependencies": {
		"@kenjiuno/decompressrtf": "^0.1.4",
		"@kenjiuno/msgreader": "^1.20.0-alpha.1",
		"@types/mailparser": "^3.4.4",
		"@types/node": "^22.5.0",
		"document-highlighter": "^2.1.0",
		"html-pdf": "^3.0.1",
		"iconv-lite": "^0.6.3",
		"mailparser": "^3.6.4",
		"rtf-stream-parser": "^3.6.0",
		"typescript": "^5.5.4",
		"@types/html-pdf": "^3.0.3",
		"pdf-lib": "^1.17.1"
	}
}
